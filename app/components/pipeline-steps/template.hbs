<div class="pipeline-steps">
  <div class="steps-line"></div>
  <div class="steps-head-dot"></div>
  <div class="steps-wrap">

    {{!-- {{#sortable-objects sortableObjectList=model sortEndAction='sortEndAction' enableSort=true}} --}}
      {{#each model as |item|}}
        {{!-- {{#draggable-object class="step-content" content=item overrideClass='sortObject' isSortable=true}} --}}
          <div class="step-content">
            <div class="step-name text-muted">test step{{item.id}}</div>
          </div>
        {{!-- {{/draggable-object}} --}}
      {{/each}}
    {{!-- {{/sortable-objects}} --}}
  </div>
  <div class="step-add step-content">
    <a href="javascript:void(0);">
      {{t 'pipelineDetail.addStep'}}
    </a>
  </div>
  
</div>
